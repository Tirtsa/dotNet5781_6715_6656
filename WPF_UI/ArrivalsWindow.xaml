<Window x:Class="WPF_UI.ArrivalsWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPF_UI"
        mc:Ignorable="d"
        Title="ArrivalsWindow" Height="450" Width="450" Loaded="Window_Loaded" Closing="Window_Closing">
    <Grid>
        <ListBox x:Name="lbTimeTable" Height="265" VerticalAlignment="Top" Margin="0,85,0,0" HorizontalAlignment="Center" Width="250"/>
        <Button x:Name="backButton" Content="חזרה" HorizontalAlignment="Center" VerticalAlignment="Bottom" Width="75" Margin="0,0,0,25" Click="backButton_Click"/>
        <TextBox x:Name="tbStation" HorizontalAlignment="Center" Height="33" TextWrapping="Wrap" VerticalAlignment="Top" Width="80" Margin="0,36,0,0" FontSize="22" TextAlignment="Center"/>
        <Label Content="-זמני הגעה ל" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="277,36,0,0" Height="33" Width="101" FontSize="16"/>
    </Grid>
</Window>
